<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CanManageUserProfileController">
   <children>
      <TextField fx:id="idField" layoutX="42.0" layoutY="42.0" onAction="#idFieldOnClick" promptText="ID" />
      <TextField fx:id="passwordField" layoutX="42.0" layoutY="91.0" onAction="#passwordFieldOnClick" promptText="Password" />
      <ComboBox fx:id="comboGroup" layoutX="42.0" layoutY="143.0" prefWidth="150.0" promptText="Group" />
      <TableView fx:id="table" layoutX="260.0" layoutY="43.0" prefHeight="200.0" prefWidth="222.0">
        <columns>
          <TableColumn fx:id="tableId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableName" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="tableGrp" prefWidth="75.0" text="Group" />
        </columns>
      </TableView>
      <Button fx:id="addUserButton" layoutX="139.0" layoutY="194.0" mnemonicParsing="false" onAction="#addUserButtonOnClick" text="Add User" />
      <Button fx:id="removeUserButton" layoutX="139.0" layoutY="231.0" mnemonicParsing="false" onAction="#removeUserButtonOnClick" text="Remove User" />
   </children>
</AnchorPane>
